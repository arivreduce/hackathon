{
  "name": "Nestorfier",
  "version": "1.0",
  "description": "Replace every image by a Nestor image in a website you visit",
  "permissions": ["declarativeNetRequest", "storage"],
  "host_permissions": ["<all_urls>"],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  },
  "action": {
    "default_popup": "index.html"
  },
  "manifest_version": 3
}


